    },

    "feature_engineering": {
        "window": 7
        
    },

    "model_training": {
        "target": "target",
        "variables": [
            "lags", "delta_sign", "weekday_vector",
       "lag_pct_^BVSP", "lag_pct_ITUB4", "lag_pct_BBDC4", "lag_pct_VALE3",
